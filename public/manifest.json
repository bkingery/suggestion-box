  "fullpage": true,
  "size": {
    "width": 2,
    "height": 2
  },
  "mapping": {
    "alias": "suggestions",
    "dataSetId": "fd0e6441-d895-44e6-a7c0-27d6dd82abcc",
    "fields": [
      {
        "alias": "id",
        "columnName": "id"
      },
      {
        "alias": "message",
        "columnName": "message"
      },
      {
        "alias": "totalPlusOne",
        "columnName": "totalPlusOne"
      }
    ]
  }
}
